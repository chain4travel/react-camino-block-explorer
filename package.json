    "scripts": {
        "build": "PUBLIC_PATH=https://suite.camino.network/explorer/ webpack --config webpack.prod.js",
        "build:dev": "PUBLIC_PATH=https://dev.suite.camino.network/explorer/ webpack --config webpack.dev.js",
        "build:stage": "PUBLIC_PATH=https://stage.suite.camino.network/explorer/ webpack --config webpack.prod.js",
        "build:start": "cd dist && PORT=5002 npx serve",
        "start": "webpack-dev-server --config webpack.local.js",
        "start:live": "webpack-dev-server --open --config webpack.local.js --live-reload",
        "checkTs": "tsc --noEmit",
        "eslint": "eslint --ext js,ts,tsx",
        "lint": "eslint --ext .ts,.tsx ./src",
        "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
        "prepare": "husky install",
        "format-check": "prettier --check '{*.js, src/**/*.{js,jsx,ts,tsx}, .github/workflows/*.{yml,yaml}}'",
